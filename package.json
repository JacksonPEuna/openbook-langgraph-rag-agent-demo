{
  "name": "openbook-langgraph-agent-demo",
  "version": "2.0.0",
  "description": "LangGraph ReAct RAG agent with AWS Bedrock Knowledge Base and Express server",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "npx tsx src/server.ts",
    "start": "node dist/server.js",
    "langgraph:dev": "npx @langchain/langgraph-cli dev --port 8123",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@arizeai/openinference-instrumentation-langchain": "^4.0.6",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.700.0",
    "@aws-sdk/client-bedrock-runtime": "^3.700.0",
    "@grpc/grpc-js": "^1.14.3",
    "@langchain/anthropic": "^0.3.34",
    "@langchain/aws": "^0.1.10",
    "@langchain/core": "^0.3.80",
    "@langchain/langgraph": "^0.3.0",
    "@langchain/langgraph-checkpoint": "^0.0.18",
    "@langchain/openai": "^0.3.17",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.211.0",
    "@opentelemetry/resources": "^2.5.0",
    "@opentelemetry/sdk-trace-node": "^2.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@langchain/langgraph-cli": "^0.0.71",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.55.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "langgraph",
    "rag",
    "bedrock",
    "knowledge-base",
    "aws",
    "budget",
    "typescript",
    "agent",
    "express"
  ]
}
